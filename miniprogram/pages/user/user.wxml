<!--pages/myInfo/index.wxml-->
<view class="container">
  <view class="userinfo">
  <!-- <button open-type="chooseAvatar" bind:chooseavatar="onChooseAvatar" > -->
    <image class="userinfo-avatar" src="{{avatarUrl}}" mode="cover"></image>
  <!-- </button> -->
    
    <text class="userinfo-nickname">{{nickname}}</text>
  </view>


  <view class="new_menu">
    <view bindtap="navigateToChangeInfo" class="new_view">
      <image class="new_img3" src="./../../image/userinfo.png" mode="aspectFit" />
      修改信息
      <image class="new_img" src="../../image/right.png" mode="aspectFit" />
    </view>
  </view>


  <view class="new_menu">
    <view bindtap="navigateToAbout" class="new_view">
      <image class="new_img2" src="./../../image/about_us.png" mode="aspectFit" />
      关于我们
      <image class="new_img" src="../../image/right.png" mode="aspectFit" />
    </view>
  </view>
</view>